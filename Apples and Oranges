int m=apples.size();
    int n=oranges.size();
    int counta=0;
    int counto=0;
    for(int i=0; i<m; i++){
       apples[i]=apples[i]+a;
       if(apples[i]>=s && apples[i]<=t){
            counta=counta+1;
        }
    }
    for(int j=0; j<n; j++){
       oranges[j]=oranges[j]+b;
       if(oranges[j]>=s && oranges[j]<=t){
            counto=counto+1;
        }
    }
    cout<<counta<<endl;
    cout<<counto<<endl;
